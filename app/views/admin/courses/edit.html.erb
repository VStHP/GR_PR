<% provide :url_path, admin_course_path %>
<% provide :class_class, warning_edit_class_course(@course) %>
<% provide :warning, warning_edit_message_course(@course) %>
<% provide :title, @course.name %>
<div class="content-header">
  <div class="header-section">
    <h1>
      <i class="gi gi-notes_2"></i><%= @course.name.capitalize %>
      <br>
      <small>Chỉnh sửa khóa thực tập</small>
    </h1>
  </div>
</div>
<ul class="breadcrumb breadcrumb-top">
  <li>Khóa thực tập</li>
  <li><%= link_to @course.name.capitalize, admin_course_path(@course) %></li>
  <li>
    <a href="">Chỉnh sửa</a>
  </li>
</ul>
<!-- END Forms General Header -->
<div class="block">
  <div class="block-title">
    <h2><strong>Khóa thực tập</strong> Thành phần</h2>
  </div>
  <%= render "form" %>
</div>
<script>$(function(){ FormsWizard.init(); });</script>
